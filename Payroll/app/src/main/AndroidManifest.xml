<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.payrollbullock.payroll">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme">
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".home.Home" />
        <activity android:name=".employee.EmployeeInformation" />
        <activity android:name=".Register" />
        <activity android:name=".event.AddEvent" />
        <activity android:name=".event.EventList" />
        <activity android:name=".event.EventShow" />
        <activity android:name=".employee.EmployeeUploadPicture" />
        <activity android:name=".employee.EmployeeAddInformation"></activity>
    </application>

</manifest>